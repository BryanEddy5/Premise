Private Sub btnOpen_Click()
    Dim CustomerFile As String
    Dim path As String

    On Error GoTo ErrorHandler:

    CustomerFile = Me.bxSpecList
    path = Core_PathwayManager.GetPathway(CustomerSpecs) & Me.Customer & "\"
    path = path & Me.bxSpecList


    Call OpenCustomerSpec(path, CustomerFile)



ErrorExit:
    Exit Sub

ErrorHandler:
    If Err.Number = 94 Then
        MsgBox "Please choose a document"
    Else
        MsgBox Err.Description & " " & Err.Number
    End If
    Resume ErrorExit



End Sub

