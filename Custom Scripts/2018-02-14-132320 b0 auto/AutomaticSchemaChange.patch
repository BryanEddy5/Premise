+) ON [PRIMARY]
+GO
+SET QUOTED_IDENTIFIER ON
+GO
+SET ANSI_NULLS ON
+GO
+
+
+CREATE TRIGGER [dbo].[Customerstrgr] ON [dbo].[Customers]
+AFTER INSERT, UPDATE 
+AS
+	BEGIN 
+		IF NOT (UPDATE(RevisedDate))
+			BEGIN
+			  UPDATE t
+			  SET t.RevisedDate = GETDATE(),t.RevisedBy = (SUSER_SNAME()) 
+			  FROM dbo.Customers as t
+			  JOIN inserted i
+			  ON i.Customer = t.Customer
+			END
+	END
+
+
+
+
+
+GO
