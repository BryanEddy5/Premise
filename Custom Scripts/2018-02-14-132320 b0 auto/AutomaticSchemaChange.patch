+[RevisedBy] [nvarchar] (20) COLLATE SQL_Latin1_General_CP1_CI_AS NULL CONSTRAINT [DF_Customers_RevisedBy] DEFAULT (suser_sname()),
+[RevisedDate] [datetime] NULL CONSTRAINT [DF_Customers_RevisedDate] DEFAULT (getdate()),
